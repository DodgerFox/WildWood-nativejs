Molodofont = 'Molodofont'
PlayfairDisplay = 'PlayfairDisplay'
Montserrat = 'Montserrat'



@font-face {
	font-family: 'Molodofont';
	src: url('../fonts/MolodofontRegular.eot');
	src: url('../fonts/MolodofontRegular.eot?#iefix') format('embedded-opentype'),
		url('../fonts/MolodofontRegular.woff2') format('woff2'),
		url('../fonts/MolodofontRegular.woff') format('woff'),
		url('../fonts/MolodofontRegular.ttf') format('truetype');
	font-weight: 400;
	font-style: normal;
}

